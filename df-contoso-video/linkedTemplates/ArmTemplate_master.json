{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-contoso-video"
		},
		"AzSQL_Contoso_Video_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzSQL_Contoso_Video'"
		},
		"Contoso Video Azure Function_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'Contoso Video Azure Function'"
		},
		"CosmosDb_Contoso_Video_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDb_Contoso_Video'"
		},
		"CosmosDb_Contoso_Video_Raw_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDb_Contoso_Video_Raw'"
		},
		"Contoso Video Azure Function_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://fn-contoso-video.azurewebsites.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-contoso-video_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzSQL_Contoso_Video_connectionString": {
						"value": "[parameters('AzSQL_Contoso_Video_connectionString')]"
					},
					"Contoso Video Azure Function_functionKey": {
						"value": "[parameters('Contoso Video Azure Function_functionKey')]"
					},
					"CosmosDb_Contoso_Video_connectionString": {
						"value": "[parameters('CosmosDb_Contoso_Video_connectionString')]"
					},
					"CosmosDb_Contoso_Video_Raw_connectionString": {
						"value": "[parameters('CosmosDb_Contoso_Video_Raw_connectionString')]"
					},
					"Contoso Video Azure Function_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('Contoso Video Azure Function_properties_typeProperties_functionAppUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "df-contoso-video_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'df-contoso-video_ArmTemplate_0')]"
			]
		}
	]
}